{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://sodalite.icgc.org/messages",
  "type": "object",
  "properties": {
    "sequencingReadSubmission": {
      "id": "/properties/sequencingReadSubmission",
      "type": "object",
      "properties": {
        "apiVersion": {
          "id": "/properties/sequencingReadSubmission/properties/apiVersion",
          "type": "string"
        },
        "submissionId": {
          "id": "/properties/sequencingReadSubmission/properties/submissionId",
          "type": "string"
        },
        "sequencingRead": {
          "id": "/properties/sequencingReadSubmission/properties/sequencingRead",
          "type": "object",
          "properties": {
            "aligned": {
              "id": "/properties/sequencingReadSubmission/properties/sequencingRead/properties/aligned",
              "type": "boolean"
            },
            "alignmentTool": {
              "id": "/properties/sequencingReadSubmission/properties/sequencingRead/properties/alignmentTool",
              "type": "string"
            },
            "insertSize": {
              "id": "/properties/sequencingReadSubmission/properties/sequencingRead/properties/insertSize",
              "type": "integer"
            },
            "libraryStrategy": {
              "id": "/properties/sequencingReadSubmission/properties/sequencingRead/properties/libraryStrategy",
              "type": "string",
              "enum": ["WGS", "WXS", "RNA-Seq", "ChIP-Seq", "miRNA-Seq", "Bisulfite-Seq", "Validation", "Amplicon", "Other" ]
            },
            "pairedEnd": {
              "id": "/properties/sequencingReadSubmission/properties/sequencingRead/properties/pairedEnd",
              "type": "boolean"
            },
            "referenceGenomeFiles": {
              "id": "/properties/sequencingReadSubmission/properties/sequencingRead/properties/referenceGenomeFiles",
              "type": "array",
              "items": {
                "id": "/properties/sequencingReadSubmission/properties/sequencingRead/properties/referenceGenomeFiles/items",
                "type": "string"
              }
            }
          },
          "required":["aligned", "alignmentTool", "insertSize", "libraryStrategy", "pairedEnd", "referenceGenomeFiles"]
        },
        "study": {
          "id": "/properties/sequencingReadSubmission/properties/study",
          "type": "object",
          "properties": {
            "studyId": {
              "id": "/properties/sequencingReadSubmission/properties/study/properties/studyId",
              "type": "string"
            },
            "name": {
              "id": "/properties/sequencingReadSubmission/properties/study/properties/name",
              "type": "string"
            },
            "organization": {
              "id": "/properties/sequencingReadSubmission/properties/study/properties/organization",
              "type": "string"
            },
            "description": {
              "id": "/properties/sequencingReadSubmission/properties/study/properties/description",
              "type": "string"
            },
            "donor": {
              "id": "/properties/sequencingReadSubmission/properties/study/properties/donor",
              "type": "object",
              "properties": {
                "donorSubmitterId": {
                  "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/donorSubmitterId",
                  "type": "string"
                },
                "donorGender": {
                  "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/donorGender",
                  "type": "string",
                  "enum": ["male", "female", "unspecified"]
                },
                "specimen": {
                  "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen",
                  "type": "object",
                  "properties": {
                    "specimenSubmitterId": {
                      "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/specimenSubmitterId",
                      "type": "string"
                    },
                    "specimenClass": {
                      "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/specimenClass",
                      "type": "string",
                      "enum": ["Normal", "Tumour", "Adjacent normal"]
                    },
                    "specimenType": {
                      "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/specimenType",
                      "type": "string",
                      "enum": [ "Normal - solid tissue",
                                "Normal - blood derived",
                                "Normal - bone marrow",
                                "Normal - tissue adjacent to primary",
                                "Normal - buccal cell",
                                "Normal - EBV immortalized",
                                "Normal - lymph node",
                                "Normal - other",
                                "Primary tumour - solid tissue",
                                "Primary tumour - blood derived (peripheral blood)",
                                "Primary tumour - blood derived (bone marrow)",
                                "Primary tumour - additional new primary",
                                "Primary tumour - other",
                                "Recurrent tumour - solid tissue",
                                "Recurrent tumour - blood derived (peripheral blood)",
                                "Recurrent tumour - blood derived (bone marrow)",
                                "Recurrent tumour - other",
                                "Metastatic tumour - NOS",
                                "Metastatic tumour - lymph node",
                                "Metastatic tumour - metastasis local to lymph node",
                                "Metastatic tumour - metastasis to distant location",
                                "Metastatic tumour - additional metastatic",
                                "Xenograft - derived from primary tumour",
                                "Xenograft - derived from tumour cell line",
                                "Cell line - derived from tumour",
                                "Primary tumour - lymph node",
                                "Metastatic tumour - other",
                                "Cell line - derived from xenograft tumour"]
                    },
                    "sample": {
                      "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample",
                      "type": "object",
                      "properties": {
                        "sampleSubmitterId": {
                          "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/sampleSubmitterId",
                          "type": "string"
                        },
                        "sampleType": {
                          "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/sampleType",
                          "type": "string",
                          "enum": [ "DNA", "FFPE DNA", "Amplified DNA", "RNA", "Total RNA", "FFPE RNA"]
                        },
                        "files": {
                          "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/files",
                          "type": "array",
                          "items": {
                            "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/files/items",
                            "type": "object",
                            "properties": {
                              "fileName": {
                                "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/files/items/properties/fileName",
                                "type": "string"
                              },
                              "fileSize": {
                                "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/files/items/properties/fileSize",
                                "type": "integer"
                              },
                              "fileType": {
                                "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/files/items/properties/fileType",
                                "type": "string",
                                "enum": ["FASTA", "FAI", "FASTQ", "BAM", "BAI", "VCF", "TBI", "IDX", "XML"]
                              },
                              "fileMd5": {
                                "id": "/properties/sequencingReadSubmission/properties/study/properties/donor/properties/specimen/properties/sample/properties/files/items/properties/fileMd5",
                                "type": "string"
                              }
                            },
                            "required":["fileName", "fileSize", "fileType", "fileMd5"]
                          }
                        }
                      },
                      "required":["sampleSubmitterId", "sampleType", "files"]
                    }
                  },
                  "required":["specimenSubmitterId", "specimenClass", "specimenType", "sample"]
                }
              },
              "required":["donorSubmitterId", "donorGender", "specimen"]
            }
          },
          "required":["studyId", "name", "organization", "description", "donor"]
        }
      },
      "required":["apiVersion", "submissionId", "sequencingRead", "study"]
    }
  },
  "required":["sequencingReadSubmission"]
}
