{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://sodalite.icgc.org/messages",
	"type": "object",
	"properties": {
		"modifySequencingRead": {
			"id": "/properties/modifySequencingRead",
			"type": "object",
			"properties": {
			  "apiVersion": {
					"id": "/properties/modifySequencingRead/properties/apiVersion",
					"type": "string"
				},
			  "submissionId": {
					"id": "/properties/modifySequencingRead/properties/submissionId",
					"type": "string"
				},
				"sequencingRead": {
					"id": "/properties/modifySequencingRead/properties/sequencingRead",
					"type": "object",
					"properties": {
						"analysisId": {
							"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/analysisId",
							"type": "string"
						},
						"aligned": {
							"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/aligned",
							"type": "boolean"
						},
						"alignmentTool": {
							"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/alignmentTool",
							"type": "string"
						},
						"insertSize": {
							"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/insertSize",
							"type": "integer"
						},
						"libraryStrategy": {
							"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/libraryStrategy",
							"type": "string",
							"enum": ["WGS", "WXS", "RNA-Seq", "ChIP-Seq", "miRNA-Seq", "Bisulfite-Seq", "Validation", "Amplicon", "Other" ]
						},
						"pairedEnd": {
							"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/pairedEnd",
							"type": "boolean"
						},
						"referenceGenomeFiles": {
							"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/referenceGenomeFiles",
							"type": "array",
							"items": {
								"id": "/properties/modifySequencingRead/properties/sequencingRead/properties/referenceGenomeFiles/items",
								"type": "string"
							}
						}
					},
					"required":["analysisId"]
				},
				"study": {
					"id": "/properties/modifySequencingRead/properties/study",
					"type": "object",
					"properties": {
						"studyId": {
							"id": "/properties/modifySequencingRead/properties/study/properties/studyId",
							"type": "string"
						},
						"donor": {
							"id": "/properties/modifySequencingRead/properties/study/properties/donor",
							"type": "object",
							"properties": {
								"donorId": {
									"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/donorId",
									"type": "string"
								},
								"specimen": {
									"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/specimen",
									"type": "object",
									"properties": {
										"specimenId": {
											"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/specimen/properties/specimenId",
											"type": "string"
										},
										"sample": {
											"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/specimen/properties/sample",
											"type": "object",
											"properties": {
												"sampleId": {
													"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/specimen/properties/sample/properties/sampleId",
													"type": "string"
												},
												"files": {
													"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/specimen/properties/sample/properties/files",
													"type": "array",
													"items": {
														"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/specimen/properties/sample/properties/files/items",
														"type": "object",
														"properties": {
															"objectId": {
																"id": "/properties/modifySequencingRead/properties/study/properties/donor/properties/specimen/properties/sample/properties/files/items/properties/objectId",
																"type": "string"
															}
														},
														"required":["objectId"]
													}
												}
											},
											"required":["sampleId"]
										}
									},
									"required":["specimenId"]
								}
							},
							"required":["donorId"]
						}
					},
					"required":["studyId", "donor"]
				}
			},
			"required":["apiVersion", "sequencingRead"]
		}
	},
	"required":["modifySequencingRead"]
}
