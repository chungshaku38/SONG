    "definitions" : {
		"expField" : {
			"type" : "object",
			"properties": {
				"type" : {
					"enum": [
						"array",
						"boolean",
						"integer",
						"null",
						"number",
						"string"
					]
				}
			}
		}
	},
	"properties": {
		"study" : {
			"type": "string",
			"minLength": 1
		},
		"analysisType" : {
			"type": "string",
			"minLength": 1
		},
		"experimentSchema" : {
			"type" : "object"
		}
	},
	"additionalProperties" : false
}
